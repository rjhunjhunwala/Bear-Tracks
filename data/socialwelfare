<tr class="even"><td class="codecol"><a href="/search/?P=SOC%20WEL%20112" title="SOCWEL112" class="bubblelink code" onclick="return showCourse(this, 'SOC WEL 112');">SOCWEL112</a></td><td>Social Welfare Policy</td><td class="hourscol">3</td></tr>
<tr class="odd"><td class="codecol"><a href="/search/?P=SOC%20WEL%20114" title="SOCWEL114" class="bubblelink code" onclick="return showCourse(this, 'SOC WEL 114');">SOCWEL114</a></td><td>Practice in Social Work</td><td class="hourscol">3</td></tr>
